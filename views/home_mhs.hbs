<div class="flex flex-col w-full max-md:max-w-full">

    <section class="flex flex-col self-end mt-2 mr-12 w-full max-w-[1066px] max-md:mr-2.5 max-md:max-w-full">
      <h2 class="self-start text-2xl font-bold leading-loose text-center text-indigo-950 max-md:ml-0.5">
        Kegiatan Kampus dan Volunteer
      </h2>
      <div class="mt-11 max-md:mt-10 max-md:max-w-full">
        <div class="flex gap-5 max-md:flex-col">
          {{#each data.kegiatan }}
            <article class="flex flex-col w-[33%] max-md:ml-0 max-md:w-full">
            <div class="flex flex-col grow pb-2 w-full bg-white rounded-xl border border-solid border-black border-opacity-10 max-md:mt-5">
              <img
                loading="lazy"
                src="https://cdn.builder.io/api/v1/image/assets/TEMP/f910b45c39d5f0e784595f31ddb80b1b29b34456488addb85881deb0c1fa7d19?placeholderIfAbsent=true&apiKey=863c3774457247cf9941fba7937e9907"
                class="object-contain w-full rounded-xl aspect-[1.86]"
                alt="LKMM-TD FTI 2023/2024 Event"
              />
              <div class="flex flex-col px-8 items-start pr-2.5 pl-9 mt-7 w-full max-md:pl-5">
                <h3 class="text-xl font-medium text-indigo-950 w-[281px]">
                  {{ this.nama_kegiatan }}
                </h3>
                <p class="self-stretch mt-4 mb-8 text-xs text-indigo-950">
                  {{ this.deskripsi }}
                </p>
                <div class="flex gap-5 justify-between mt-11 w-full max-md:mt-10">
                  <button
                    class="flex gap-2 self-start text-base justify-center items-center font-medium text-center text-cyan-600"
                    tabindex="0"
                    type="button"
                    class="block"
                    aria-haspopup="dialog"
                    aria-expanded="false"
                    aria-controls="hs-ai-invoice-modal"
                    data-hs-overlay="#hs-ai-invoice-modal"
                  >
                    <span class="grow">Lihat Kegiatan</span>
                    <img
                      loading="lazy"
                      src="https://cdn.builder.io/api/v1/image/assets/TEMP/f67b8f6518d4f67ea7e77741354d108243e0a07b2d32c13d9c8e96bc3659500e?placeholderIfAbsent=true&apiKey=863c3774457247cf9941fba7937e9907"
                      class="object-contain shrink-0 my-auto w-2.5 aspect-square"
                      alt=""
                    />
                  </button>
                  <div class="flex flex-col whitespace-nowrap">
                    <a class="self-start px-5 pt-1.5 pb-3 text-xs font-medium bg-cyan-600 rounded-xl text-neutral-100 max-md:pl-5 max-md:ml-2.5" href="/mahasiswa/daftar?user={{ ../data.user.user_id }}&kegiatan={{ this.kegiatan_id }}" tabindex="0">
                      Daftar
                    </a>
                    <time datetime="2023-11-10" class="mt-1 text-xs text-indigo-950">{{ this.tanggal_mulai }} - {{ this.tanggal_selesai }}</time>
                  </div>
                </div>
              </div>
            </div>
          </article>
          {{/each}}
          
          <button class="self-center mt-5 text-xl leading-3 text-center text-cyan-600" tabindex="0">Load more</button>
          
