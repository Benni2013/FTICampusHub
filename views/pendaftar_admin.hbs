<div>

  <!-- Card -->
  <div class="flex flex-col">
    <div class="-m-1.5 overflow-x-auto">
      <h1 class="text-lg text-bold p-4">Pendaftar</h1>
      <div class="p-1 min-w-full inline-block align-middle">
        
        <div class="bg-white border border-gray-200 rounded-xl shadow-sm overflow-hidden">
          
          <!-- Table -->
          <table class="min-w-full divide-y divide-gray-200">
            <thead class="bg-white-200">
              <tr>
                <th class="px-2 py-3 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Nomor Anggota</span>
                  </div>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Nama Anggota</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Nama Kegiatan</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Tanggal Pendaftaran</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Penyelenggara</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Divisi</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Aksi</span>
                </th>
              </tr>
            </thead>
            <tbody class="divide-y divide-gray-200">
              {{#each data.daftar}}
                <form method="POST" action="/penyelenggara/penerimaan?penyelenggara={{../data.penyelenggara.penyelenggara_id}}&kegiatan={{this.kegiatan_id}}">
                  <tr class="bg-white hover:bg-gray-50">
                    <td class="px-6 py-2">
                      <span class="font-mono text-sm text-blue-600">{{this.nim}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.nama}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.nama_kegiatan}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.tanggal_mulai}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.nama_penyelenggara}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.jabatan}}</span>
                    </td>
                    <td class="size-px whitespace-nowrap">
                      <button type="button" class="block" aria-haspopup="dialog" aria-expanded="false" aria-controls="hs-ai-invoice-modal" data-hs-overlay="#hs-ai-invoice-modal">
                    <span class="px-6 py-1.5">
                      <span
                        class="py-1 px-2 inline-flex justify-center items-center gap-2 rounded-lg border font-medium bg-white text-gray-700 shadow-sm align-middle hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-offset-white focus:ring-blue-600 transition-all text-sm"
                      >
                        <svg class="shrink-0 size-4" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                          <path
                            d="M1.92.506a.5.5 0 0 1 .434.14L3 1.293l.646-.647a.5.5 0 0 1 .708 0L5 1.293l.646-.647a.5.5 0 0 1 .708 0L7 1.293l.646-.647a.5.5 0 0 1 .708 0L9 1.293l.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .801.13l.5 1A.5.5 0 0 1 15 2v12a.5.5 0 0 1-.053.224l-.5 1a.5.5 0 0 1-.8.13L13 14.707l-.646.647a.5.5 0 0 1-.708 0L11 14.707l-.646.647a.5.5 0 0 1-.708 0L9 14.707l-.646.647a.5.5 0 0 1-.708 0L7 14.707l-.646.647a.5.5 0 0 1-.708 0L5 14.707l-.646.647a.5.5 0 0 1-.708 0L3 14.707l-.646.647a.5.5 0 0 1-.801-.13l-.5-1A.5.5 0 0 1 1 14V2a.5.5 0 0 1 .053-.224l.5-1a.5.5 0 0 1 .367-.27zm.217 1.338L2 2.118v11.764l.137.274.51-.51a.5.5 0 0 1 .707 0l.646.647.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.509.509.137-.274V2.118l-.137-.274-.51.51a.5.5 0 0 1-.707 0L12 1.707l-.646.647a.5.5 0 0 1-.708 0L10 1.707l-.646.647a.5.5 0 0 1-.708 0L8 1.707l-.646.647a.5.5 0 0 1-.708 0L6 1.707l-.646.647a.5.5 0 0 1-.708 0L4 1.707l-.646.647a.5.5 0 0 1-.708 0l-.509-.51z"
                          />
                          <path
                            d="M3 4.5a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 0 1 0 1h-6a.5.5 0 0 1-.5-.5zm8-6a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5z"
                          />
                        </svg>
                        Detail
                      </span>
                    </span>
                  </button>
                    </td>
                  </tr>
<div id="hs-ai-invoice-modal" class="hs-overlay hidden size-full fixed m-10 p-10 top-0 start-0 z-[80] overflow-x-hidden overflow-y-auto pointer-events-none" role="dialog" tabindex="-1" aria-labelledby="hs-ai-invoice-modal-label">
            <div class="hs-overlay-open:mt-7 hs-overlay-open:opacity-100 hs-overlay-open:duration-500 mt-0 opacity-0 ease-out transition-all sm:max-w-lg sm:w-full sm:mx-auto">
              <div class="relative flex flex-col bg-white shadow-lg  pointer-events-auto">
                <div class="relative overflow-hidden min-h-32 bg-gray-900 text-center ">
                  <div class="flex flex-wrap gap-5 justify-between items-start text-3xl ps-5 py-5 font-bold text-white max-md:max-w-full">
                    <div class="mt-3.5">Lihat Detail</div>
                  </div>
                  <!-- Close Button -->
                  <div class="absolute top-2 end-2">
                    <button
                      type="button"
                      class="flex justify-center items-center size-7 text-sm font-semibold rounded-full border border-transparent text-white/70 hover:bg-white/10 focus:outline-none focus:bg-white/10 disabled:opacity-50 disabled:pointer-events-none"
                      aria-label="Close"
                      data-hs-overlay="#hs-ai-invoice-modal"
                    >
                      <span class="sr-only">Close</span>
                      <svg class="shrink-0 size-4" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path
                          d="M18 6 6 18"
                        /><path d="m6 6 12 12" /></svg>
                    </button>
                  </div>
                  <!-- End Close Button -->

                  <!-- SVG Background Element -->
                  <figure class="absolute inset-x-0 bottom-0">
                    <svg preserveAspectRatio="none" xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" viewBox="0 0 1920 100.1">
                      <path fill="currentColor" class="fill-white" d="M0,0c0,0,934.4,93.4,1920,0v100.1H0L0,0z"></path>
                    </svg>
                  </figure>
                  <!-- End SVG Background Element -->
                </div>

                <form class="flex flex-col text-xl px-2 mx-2 font-semibold text-white rounded-none max-w-[949px]">
                  <div class="flex flex-col px-12 py-14 w-full bg-gray-700 rounded-3xl max-md:px-5 max-md:max-w-full">
                    <label for="nama" class="self-start ml-3.5 max-md:ml-2.5">Nama</label>
                    <input type="text" id="nama" value="{{this.nama}}" name="nama" class=" h-[46px] px-6 pt-2.5 pb-6 mt-3.5 bg-white rounded-3xl text-black text-opacity-50 max-md:px-5 max-md:max-w-full" aria-label="Nama input field" disabled />

                    <label for="nim" class="self-start mt-5 ml-3.5 max-md:ml-2.5">NIM</label>
                    <input type="text" id="nim" value="{{this.nim}}" name="nim" class="h-[46px] px-6 pt-2.5 pb-6 mt-3.5 bg-white rounded-3xl text-black text-opacity-50 max-md:px-5 max-md:max-w-full" aria-label="NIM input field" disabled />

                    <label for="Email" class="self-start mt-5 ml-3.5 max-md:ml-2.5">Email</label>
                    <input type="text" id="Email" value="{{this.email}}" name="Email" class="h-[46px] px-6 pt-2.5 pb-6 mt-3.5 bg-white rounded-3xl text-black text-opacity-50 max-md:px-5 max-md:max-w-full" aria-label="Email input field" disabled />

                    <label for="No HP" class="self-start mt-5 ml-3.5 max-md:ml-2.5">No HP</label>
                    <input type="text" id="No HP" value="{{this.no_telp}}" name="No HP" class="h-[46px] px-6 pt-2.5 pb-6 mt-3.5 bg-white rounded-3xl text-black text-opacity-50 max-md:px-5 max-md:max-w-full" aria-label="No HP input field" disabled />

                    <label label for="Alamat" class="self-start mt-5 ml-3.5 max-md:ml-2.5">Alamat</label>
                    <input type="text" id="Alamat" value="{{this.alamat}}" name="Alamat" class="h-[46px] px-6 pt-2.5 pb-6 mt-3.5 bg-white rounded-3xl text-black text-opacity-50 max-md:px-5 max-md:max-w-full" aria-label="Alamat input field" disabled />

                    <label for="position" class="self-start mt-6 ml-3.5 max-md:ml-2.5">Jabatan</label>
                    <input type="text" id="Alamat" value="{{this.jabatan}}" name="Alamat" class="h-[46px] px-6 pt-2.5 pb-6 mt-3.5 bg-white rounded-3xl text-black text-opacity-50 max-md:px-5 max-md:max-w-full" aria-label="Alamat input field" disabled />
                    <label for="position" class="self-start mt-6 ml-3.5 max-md:ml-2.5">Alasan dan Pengalamana</label>
                    <input type="text" id="Alamat" value="{{this.alasan_pendaftaran}}" name="Alamat" class="h-[46px] px-6 pt-2.5 pb-6 mt-3.5 bg-white rounded-3xl text-black text-opacity-50 max-md:px-5 max-md:max-w-full" aria-label="Alamat input field" disabled />

          
                    <div class="flex gap-5 justify-between pt-2 px-3 self-center mt-11 max-w-full whitespace-nowrap w-[365px] max-md:mt-10">
                      <input type="hidden" name="kegiatan_id" value="{{this.kegiatan_id}}" />
                      <input type="hidden" name="user_id" value="{{this.user_id}}" />
                      <button class="px-3 py-2 bg-green-500 text-white rounded" name="action" value="confirm" type="submit">
                        Konfirmasi
                      </button>
                      <button class="px-3 py-2 bg-red-500 text-white rounded" name="action" value="late" type="submit">
                        Tolak
                      </button>
                    </div>
                  </div>
                </form>
                <!-- End Body -->
              </div>
            </div>
          </div>
                </form>

              {{/each}}
            </tbody>
          </table>
          <!-- End Table -->

        </div>
      </div>
    </div>
    <div class="-m-1.5 overflow-x-auto">
      <h1 class="text-lg text-bold p-4 mt-8">Panitia</h1>
      <div class="p-1 min-w-full inline-block align-middle">
        
        <div class="bg-white border border-gray-200 rounded-xl shadow-sm overflow-hidden">
          
          <!-- Table -->
          <table class="min-w-full divide-y divide-gray-200">
            <thead class="bg-white-200">
              <tr>
                <th class="px-2 py-3 text-start">
                  <div class="flex items-center gap-x-2">
                    <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Nomor Anggota</span>
                  </div>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Nama Anggota</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Nama Kegiatan</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Tanggal Pendaftaran</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Penyelenggara</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Divisi</span>
                </th>
                <th class="px-6 py-3 text-start">
                  <span class="text-xs font-semibold uppercase tracking-wide text-gray-800">Aksi</span>
                </th>
              </tr>
            </thead>
            <tbody class="divide-y divide-gray-200">
              {{#each data.hasil}}
                <form method="POST" action="/penyelenggara/penerimaan?penyelenggara={{../data.penyelenggara.penyelenggara_id}}&kegiatan={{this.kegiatan_id}}">
                  <tr class="bg-white hover:bg-gray-50">
                    <td class="px-6 py-2">
                      <span class="font-mono text-sm text-blue-600">{{this.nim}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.nama}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.nama_kegiatan}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.tanggal_mulai}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.nama_penyelenggara}}</span>
                    </td>
                    <td class="px-6 py-2">
                      <span class="text-sm text-gray-600">{{this.jabatan}}</span>
                    </td>
                    <td class="size-px whitespace-nowrap">
                      <span class="text-sm text-gray-600">{{this.status}}</span>
                      
                    </td>
                  </tr>

                </form>

              {{/each}}
            </tbody>
          </table>
          <!-- End Table -->

        </div>
      </div>
    </div>
  </div>
  <!-- End Card -->
</div>