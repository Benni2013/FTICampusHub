<div class="certificate">
  <div class="header">
    <div class="logo">
      <img src="path/to/logo.png" alt="Logo" />
    </div>
    <h1>PT DU AZ SOLUSI</h1>
  </div>
  <div class="content">
    {{#each sertifikatList}}
      <h2>{{this.panitia_kegiatan.User.nama}}</h2> <!-- Nama Panitia -->
      <p>telah mengikuti dengan baik</p>
      <h3>{{this.panitia_kegiatan.Kegiatan.nama_kegiatan}}</h3> <!-- Nama Kegiatan -->
      <p>peserta dengan hasil/predikat</p>
      <h4>{{this.predikat}}</h4> <!-- Status Predikat -->
      <p>LULUS</p>
      <p>Terimakasih atas prestasi yang dicapai</p>
    {{/each}}
  </div>
  <div class="footer">
    <div class="instructors">
      <p>{{nomor_sertifikat}}</p>
      <p>{{tanggal_terbit}}</p>
    </div>
    <div class="qr-code">
      <img src="{{this.qr_code}}" alt="QR Code">
    </div>
  </div>
  <button onclick="window.print();">Download Sertifikat</button>
</div>